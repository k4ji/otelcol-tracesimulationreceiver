{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "labels": [
    "renovatebot",
    "dependencies"
  ],
  "enabledManagers": [
    "dockerfile",
    "regex",
    "custom"
  ],
  "extends": [
    "config:recommended",
    "schedule:daily"
  ],
  "customManagers": [
    {
      "description": "Parse Go modules in manifest.yaml",
      "fileMatch": [
        "^manifest\\.yaml$"
      ],
      "matchStrings": [
        "gomod:\\s*(?<depName>[^\\s]+)\\s*(?<currentValue>v?[0-9\\.]+)"
      ],
      "datasourceTemplate": "go",
      "depNameTemplate": "{{depName}}"
    }
  ],
  "regexManagers": [
    {
      "fileMatch": [
        "^Dockerfile$"
      ],
      "matchStrings": [
        "git clone --depth=1 --branch (?<currentValue>v?[0-9.]+) https://github\\.com/k4ji/tracesimulationreceiver\\.git"
      ],
      "datasourceTemplate": "github-tags",
      "depNameTemplate": "k4ji/tracesimulationreceiver",
      "versioningTemplate": "semver"
    }
  ],
  "packageRules": [
    {
      "managers": [
        "custom"
      ],
      "matchPackagePatterns": [
        "^github\\.com/k4ji/tracesimulationreceiver$"
      ],
      "groupName": "TraceSimulationReceiver updates"
    },
    {
      "managers": [
        "regex"
      ],
      "matchPackageNames": [
        "k4ji/tracesimulationreceiver"
      ],
      "groupName": "TraceSimulationReceiver updates"
    },
    {
      "managers": [
        "custom"
      ],
      "excludePackagePatterns": [
        "^github\\.com/k4ji/tracesimulationreceiver$"
      ],
      "groupName": "OpenTelemetry Collector module updates"
    },
    {
      "matchManagers": [
        "github-actions"
      ],
      "groupName": "github-actions deps"
    }
  ],
  "prConcurrentLimit": 10,
  "suppressNotifications": [
    "prEditedNotification"
  ]
}